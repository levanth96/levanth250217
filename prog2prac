#include <stdio.h>
int main() {
 int N, weight, accepted_count , rejected_count ;
    printf("enter a number of products ");
    scanf("%d", &N);

    printf("enter weights");
    for (int i = 0; i < N; i++) {
        scanf("%d", &weight);
        
        if (weight >= 950 && weight <= 1050) {
           printf("accepted");
        } else {
            printf("rejected");
        }
    }
    printf("Accepted Products%d\n", accepted_count);
    printf("Rejected Products%d\n", rejected_count);

    return 0;
}
